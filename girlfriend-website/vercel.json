{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "images/**",
      "use": "@vercel/static"
    },
    {
      "src": "song/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/page1",
      "dest": "/page1.html"
    },
    {
      "src": "/page2",
      "dest": "/page2.html"
    },
    {
      "src": "/page3",
      "dest": "/page3.html"
    },
    {
      "src": "/page4",
      "dest": "/page4.html"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/song/(.*)",
      "dest": "/song/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 